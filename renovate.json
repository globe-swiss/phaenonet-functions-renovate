{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"config:best-practices",
		":disableDependencyDashboard"
	],
	"enabledManagers": [
		"pep621",
		"github-actions"
	],
	"uv": {
		"fileMatch": [
			"^uv.lock$"
		]
	},
	"automerge": false,
	"automergeType": "branch"
}
