{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"config:best-practices"
	],
	"enabledManagers": [
		"pep621",
		"github-actions"
	],
	"packageRules": [
	  {
	    "matchManagers": ["pep621"],
	    "matchDepTypes": ["requires-python"],
	    "enabled": false
	  }
	],
	"automerge": false,
	"automergeType": "branch"
}
